{%- for attr in attributes -%}
  {%- if attr.has_association? != false -%}
    {%- capture name_col -%}
      {{ attr.name_ns }}:{{ attr.name }}
      ({{ attr.gen_name }})
    {%- endcapture -%}
    | {{ name_col | newline_to_br }}
    | {{ attr.type_ns }}:{{ attr.type }} [{{ attr.cardinality }}] 
    | {{ attr.definition }}
  {%- endif -%}
{%- endfor -%}